install.packages("dplyr")library(dplyr)sessionInfo()mtcars %>% head()mtcars %>% nrow()mtcars %>% ncol()mtcars %>% dim()mtcars %>% names()mtcars %>% class()mtcars %>% summary()iris %>% head()iris %>% summary()library(dplyr)iris %>% glimpse()iris2 <- iris %>% filter(iris, Species =="Setosa")iris2 %>% dim()filter(iris, Species =="Setosa")iris2<- iris %>% filter(Species=="Setosa")iris3<- iris %>% filter(Sepal.Length>5.6)iris3 %>% dim()iris %>% filter(Sepal.Length>5.2)%>% dim()history(60)iris %>% group_by(Species)%>% summarise(mean(Sepal.Length),var(Sepal.Length),sd(Sepal.Length))iris %>% group_by(Species)%>% summarise(varsl=var(Sepal.Length))mtcars %>% group_by(cyl)%>% summarise(meanmpg=mean(mpg))mtcars %>% group_by(cyl)%>% summarise(mean(mpg))mtcars %>% group_by(vs,am)%>% summarise(median(mpg))help(mtcars)mtcars %>% select(1:4)%>% head(4)mtcars %>% select(1:drat)mtcars %>% select(cyl:vs)mtcars %>% select(2:4, everything ())%>% head(4)mtcars %>% mutate(sqrt(mpg),log(mpg)) %>% select(mpg,sqrt(mpg),log(mpg), everything())mtcars %>% mutate(sqrt(qsec))mtcars %>% mutate(sqrt(mpg),log(mpg),mpg)mtcars %>% select(sqrt(mpg),log(mpg), everything())
